<div class="admin-main-container">


	<%= render 'shared/vipnavbar'%>

	<div class="admin-right-frame">
		
		<%= render 'shared/viprightframeheader'%>

		<div class="admin-right-frame-content">






<h2>All fixtures in DB</h2>

<% @fixtures.each do |fixture| %>
    [<%= link_to "destroy", fixture_path(fixture.id), method: :delete, data: { confirm: 'Are you sure you want to delete this fixture?' }%>]
    [<%= link_to "edit", edit_fixture_path(fixture.id) %>] 
    <%= fixture.id %>
    â€¢ <b> <%= fixture.competseason.competition.name %> Round <%= fixture.round %> </b> 
    <small>(<%= fixture.date %>)</small> |
    <%= link_to fixture_path(fixture.id), target: :_blank do %>
        
        <%= fixture.hometeam.display_name %>
        
        <%if !fixture.scorehome.nil? && !fixture.scoreaway.nil? %>
            <b> <%= fixture.scorehome%> - <%= fixture.scoreaway%> </b>
        <%else%>
            vs.
        <%end%>

        <%= fixture.awayteam.display_name %>

    <%end%> <br>
<%end%>







